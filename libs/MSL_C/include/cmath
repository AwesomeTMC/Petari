#ifndef CMATH_H
#define CMATH_H

#ifdef __cplusplus
namespace std {
extern "C" {
#endif

double atan2(double, double);
double ceil(double);
double floor(double);
double frexp(double, int *);
double ldexp(double, int);
double sqrt(double);

double scalbn(double, int);

int __float_huge[];
int __float_nan[];

#define INFINITY (*(float*) __float_huge)	
#define NAN (*(float*) __float_nan)

#ifdef __cplusplus
};
};
#endif

#endif // CMATH_H